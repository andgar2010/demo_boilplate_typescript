{
  "name": "boilplate_typescript",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "prebuild": "rimraf dist/*",
    "predebug": "rimraf dist/*",
    "build": "tsc",
    "prestart": "npm run build",
    "lint": "eslint --fix ./src/*",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpileOnly src/app.ts",
    "dev:debug": "ts-node-dev --inspect=4321 --respawn --transpileOnly src/index.ts",
    "dev:inspect_cmd": "nodemon --inspect-brk --inspect=0.0.0.0:9229 src/app.ts",
    "dev:inspect_nodemon": "nodemon --inspect src/index.ts",
    "dev:nodemon": "nodemon src/index.ts",
    "debug": "tsc-watch --onSuccess \"node -r module-alias/register dist/app.js\" --onFailure \"echo WHOOPS! Server compilation failed\""
  },
  "keywords": [],
  "author": "Andres Garcia",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3"
  },
  "_moduleAliases": {
    "@shared": "dist/"
  }
}
